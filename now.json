{
  "github": {
    "enabled": true
  },
  "trailingSlash": true,
  "rewrites": [
    { "source": "/(.*)", "destination": "404" }
  ],
  {
  "redirects": [
    { "source": "/services/site", "destination": "/static-site" }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": { "distDir": "dist" }
    }
  ]
}
