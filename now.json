{
  "github": {
    "enabled": true
  },
  "trailingSlash": true,
  "rewrites": [
    { "source": "/(.*)", "destination": "404" }
  ],
  "redirects": [
    { "source": "/services/site", "destination": "/static-site" }
  ],
  "builds": [
    {
      "src": "api/index.js",
      "use": "@now/node"
    },
    {
      "src": "api/actions/**/*.js",
      "use": "@now/node"
    },
    {
      "src": "api/get/**/*.ts",
      "use": "@now/node"
    }
  ]
}
