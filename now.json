{
  "name": "sergeyfilimonov.com",
  "version": 2,
  "github": { "enabled": false },
  "routes": [
    { "handle": "filesystem" },
    { "src": "/.*", "status": 404, "dest": "404" }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": { "distDir": "dist" }
    },
    {
      "src": "api/**/*.js",
      "use": "@now/node"
    }
  ]
}
