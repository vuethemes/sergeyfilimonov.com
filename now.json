{
  "scope": "sfilimonov",
  "github": { "enabled": true },
  "trailingSlash": true,
  "rewrites": [
    { "source": "/(.*)", "destination": "404" },
    { "source": "/services/site/", "destination": "/static-site" }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": { "distDir": "dist" }
    }
  ]
}
