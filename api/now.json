{
  "github": { "enabled": false },
  "routes": [
    {
      "src": "/get/(.*)",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ],
  "builds": [
    {
      "src": "index.ts",
      "use": "@now/node"
    },
    {
      "src": "actions/**/*.js",
      "use": "@now/node"
    },
    {
      "src": "get/**/*.ts",
      "use": "@now/node"
    }
  ]
}
